                        <a data-page="cv" {{#cv_active}}class="active"{{/cv_active}} href="/cv"><div class="menu-item"><span class="icon icon-cv"></span><span class="menu-text">CV</span></div></a>
                        <a data-page="blog" {{#blog_active}}class="active"{{/blog_active}} href="/blog"><div class="menu-item"><span class="icon icon-blog"></span><span class="menu-text">BLOG</span></div></a>
